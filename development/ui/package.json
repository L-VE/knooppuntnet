{
  "name": "knooppuntnet",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "extract-i18n": "ng xi18n --output-path locale && xliffmerge --profile xliffmerge.json nl de fr",
    "start": "ng serve --open --proxy-config src/proxy.conf.js --host 0.0.0.0",
    "start:nl": "ng serve --configuration=nl --open --proxy-config src/proxy.conf.js --host 0.0.0.0",
    "start:de": "ng serve --configuration=de --open --proxy-config src/proxy.conf.js --host 0.0.0.0",
    "start:fr": "ng serve --configuration=fr --open --proxy-config src/proxy.conf.js --host 0.0.0.0",
    "test-no-watch": "ng test --single-run",
    "build-dev": "ng build --progress --output-path ../public",
    "build-prod": "ng build --base-href /en/ --extract-css --progress --prod --aot --output-path ../public/en",
    "build-prod:nl": "ng build --base-href /nl/ --configuration=production-nl --extract-css --progress --prod --aot --output-path ../public/nl",
    "build-prod:de": "ng build --base-href /de/ --configuration=production-de --extract-css --progress --prod --aot --output-path ../public/de",
    "build-prod:fr": "ng build --base-href /fr/ --configuration=production-fr --extract-css --progress --prod --aot --output-path ../public/fr",
    "build-prod-with-map": "ng build --base-href /en/ --extract-css --progress --prod --aot --output-path ../public/with-map --stats-json --source-map"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.3",
    "@angular/cdk": "^7.3.0",
    "@angular/common": "^7.2.3",
    "@angular/compiler": "^7.2.3",
    "@angular/core": "^7.2.3",
    "@angular/forms": "^7.2.3",
    "@angular/http": "^7.2.3",
    "@angular/material": "^7.2.2",
    "@angular/platform-browser": "^7.2.3",
    "@angular/platform-browser-dynamic": "^7.2.3",
    "@angular/router": "^7.2.3",
    "@types/file-saver": "^2.0.0",
    "canvg": "^1.5.3",
    "core-js": "^3.0.1",
    "file-saver": "^2.0.1",
    "hammerjs": "^2.0.8",
    "immutable": "^4.0.0-rc.12",
    "jspdf": "^1.5.3",
    "ngx-cookie-service": "^2.1.0",
    "ngx-i18nsupport": "^0.17.1",
    "ngx-markdown": "^7.1.3",
    "ol": "^5.3.0",
    "rxjs": "^6.4.0",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.8",
    "@angular/cli": "^7.3.0",
    "@angular/compiler-cli": "^7.2.3",
    "@angular/language-service": "^7.2.3",
    "@types/jasmine": "^3.3.12",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "~11.13.2",
    "codelyzer": "~5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.2",
    "svg-inline-loader": "^0.8.0",
    "typescript": ">=3.1.1 <3.3",
    "ts-node": "~8.0.3",
    "tslint": "c5.15.0"
  },
  "cli": {
    "packageManager": "yarn"
  }
}
